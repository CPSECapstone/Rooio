<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ECF0F1"
    tools:context=".AddLocationLogin">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="826dp"
        android:layout_height="524dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="182dp"
        android:background="@drawable/shadow"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintStart_toEndOf="@+id/guideline5"
        app:layout_constraintTop_toBottomOf="@+id/guideline3"
        app:layout_constraintVertical_bias="0.245">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="817dp"
            android:layout_height="119dp"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="3dp"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/service_location"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.499"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.379" />

            <ImageView
                android:id="@+id/greenBar"
                android:layout_width="99dp"
                android:layout_height="3dp"
                android:layout_marginStart="436dp"
                android:layout_marginTop="7dp"
                android:layout_marginEnd="436dp"
                android:layout_marginBottom="17dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/title"
                app:layout_constraintVertical_bias="0.518"
                app:srcCompat="@color/colorAccent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout4"
            android:layout_width="823dp"
            android:layout_height="387dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="35dp"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="610dp"
                android:text="@string/address"
                android:textColor="#A3A3A3"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.022"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/add_location"
                android:layout_width="219dp"
                android:layout_height="48dp"
                android:layout_marginStart="150dp"
                android:layout_marginBottom="112dp"
                android:background="@drawable/green_button_border"
                android:fontFamily="monospace"
                android:text="@string/add_location"
                android:textAllCaps="false"
                android:textColor="#06C47B"
                android:textColorHighlight="#00F8F6F6"
                android:textColorHint="#00FCFBFB"
                android:textColorLink="#00FDFCFC"
                android:textSize="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/new_location"
                android:layout_width="747dp"
                android:layout_height="54dp"
                android:layout_marginStart="27dp"
                android:layout_marginEnd="29dp"
                android:layout_marginBottom="72dp"
                android:background="@drawable/edittextbackground"
                app:layout_constraintBottom_toTopOf="@+id/add_location"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.514"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/errorMessage"
                android:layout_width="291dp"
                android:layout_height="30dp"
                android:autoText="false"
                android:text="TextView"
                android:textAlignment="center"
                android:textColor="#E4E40B0B"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.49"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.100000024" />

            <Button
                android:id="@+id/cancel"
                android:layout_width="241dp"
                android:layout_height="48dp"
                android:layout_marginEnd="150dp"
                android:layout_marginBottom="112dp"
                android:background="@drawable/gray_button_border"
                android:fontFamily="@font/nunito_sans"
                android:text="Cancel"
                android:textAllCaps="false"
                android:textColor="#919191"
                android:textColorHighlight="#00F8F6F6"
                android:textColorHint="#00FCFBFB"
                android:textColorLink="#00FDFCFC"
                android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="150dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="670dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="535dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="1385dp" />
</androidx.constraintlayout.widget.ConstraintLayout>