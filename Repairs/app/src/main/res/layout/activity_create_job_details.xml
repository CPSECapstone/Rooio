<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ECF0F1"
    tools:context=".Login">

    <ScrollView
        android:id="@+id/jobDetailScrollView"
        style="@android:style/Widget.ScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/jobDetailLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/viewEquipment"
                android:layout_width="400dp"
                android:layout_height="36dp"
                android:fontFamily="@font/nunito_sans"
                android:text="@string/click_view_equipment"
                android:textAlignment="center"
                android:textColor="#6D7377"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.8" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/equipmentLayout"
                android:layout_width="520dp"
                android:layout_height="395dp"
                android:layout_marginTop="1150dp"
                android:background="@drawable/green_button_border"
                android:translationY="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <TextView
                    android:id="@+id/manufacturerText"
                    android:layout_width="150dp"
                    android:layout_height="25dp"
                    android:fontFamily="@font/nunito"
                    android:text="@string/manufacturer"
                    android:textAllCaps="true"
                    android:textColor="#BEC3C7"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.27" />

                <TextView
                    android:id="@+id/modelText"
                    android:layout_width="150dp"
                    android:layout_height="25dp"
                    android:fontFamily="@font/nunito"
                    android:text="@string/model_number"
                    android:textAllCaps="true"
                    android:textColor="#BEC3C7"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.75"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.27" />

                <TextView
                    android:id="@+id/serialText"
                    android:layout_width="150dp"
                    android:layout_height="25dp"
                    android:fontFamily="@font/nunito"
                    android:text="@string/serial_number"
                    android:textAllCaps="true"
                    android:textColor="#BEC3C7"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.75"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias=".52" />

                <TextView
                    android:id="@+id/lastServiceDateText"
                    android:layout_width="175dp"
                    android:layout_height="25dp"
                    android:fontFamily="@font/nunito"
                    android:text="@string/last_service_date"
                    android:textAllCaps="true"
                    android:textColor="#BEC3C7"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.805"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.77" />

                <TextView
                    android:id="@+id/locationText"
                    android:layout_width="150dp"
                    android:layout_height="25dp"
                    android:fontFamily="@font/nunito"
                    android:text="@string/location"
                    android:textAllCaps="true"
                    android:textColor="#BEC3C7"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias=".52" />

                <TextView
                    android:id="@+id/lastServiceByText"
                    android:layout_width="150dp"
                    android:layout_height="25dp"
                    android:fontFamily="@font/nunito"
                    android:text="@string/last_service_by"
                    android:textAllCaps="true"
                    android:textColor="#BEC3C7"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.77" />

                <ImageView
                    android:id="@+id/equipmentDivider"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:contentDescription="@string/divider"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.2"
                    app:srcCompat="@color/colorGray" />

                <TextView
                    android:id="@+id/equipmentName"
                    android:layout_width="400dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textColor="@color/colorPrimary"
                    android:textSize="27sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <ImageView
                    android:id="@+id/dropDown"
                    android:layout_width="18dp"
                    android:layout_height="9dp"
                    android:layout_marginTop="40dp"
                    android:contentDescription="@string/drop_down_arrow"
                    android:scaleX="1.25"
                    android:scaleY="1.25"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.95"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@drawable/details" />

                <TextView
                    android:id="@+id/manufacturerInfo"
                    android:layout_width="210dp"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.37" />

                <TextView
                    android:id="@+id/locationInfo"
                    android:layout_width="210dp"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.63" />

                <TextView
                    android:id="@+id/lastServiceByInfo"
                    android:layout_width="210dp"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.12"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.89" />

                <TextView
                    android:id="@+id/modelInfo"
                    android:layout_width="210dp"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.895"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.37" />

                <TextView
                    android:id="@+id/serialInfo"
                    android:layout_width="210dp"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.895"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias=".63" />

                <TextView
                    android:id="@+id/lastServiceDateInfo"
                    android:layout_width="210dp"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.895"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.89" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/jobDetailInfoLayout"
                android:layout_width="521dp"
                android:layout_height="964dp"
                android:layout_marginTop="150dp"
                android:layout_marginBottom="495dp"
                android:background="@drawable/shadow"
                android:paddingBottom="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.73">

                <Button
                    android:id="@+id/sendRequestButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="240dp"
                    android:layout_height="55dp"
                    android:background="@drawable/green_button_border"
                    android:fontFamily="@font/nunito_sans"
                    android:text="@string/send_request"
                    android:textAllCaps="false"
                    android:textColor="@color/colorPrimary"
                    android:textSize="24sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.93" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/fieldsConstraint"
                    android:layout_width="412dp"
                    android:layout_height="684dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/whatHappenedInput"
                        android:layout_width="match_parent"
                        android:layout_height="121dp"
                        android:background="@drawable/edittextbackground"
                        android:fontFamily="@font/nunito"
                        android:gravity="top|left"
                        android:padding="10dp"
                        android:textAlignment="viewStart"
                        android:textColor="#6D7377"
                        android:textSize="26sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.28" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/phoneNumberInput"
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:background="@drawable/edittextbackground"
                        android:fontFamily="@font/nunito"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:textAlignment="textStart"
                        android:textColor="#6D7377"
                        android:textSize="26sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.97" />

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/contactInput"
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:background="@drawable/edittextbackground"
                        android:fontFamily="@font/nunito"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:textAlignment="textStart"
                        android:textColor="#6D7377"
                        android:textSize="26sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.758" />

                    <TextView
                        android:id="@+id/whatHappenedText"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="@font/nunito"
                        android:text="@string/what_happened"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.18" />

                    <TextView
                        android:id="@+id/serviceText"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:fontFamily="@font/nunito"
                        android:text="@string/service_type"
                        android:textAlignment="textStart"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <Spinner
                        android:id="@+id/serviceTypeSpinner"
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:background="@drawable/spinner_background"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.060000002" />

                    <Spinner
                        android:id="@+id/monthSpinner"
                        android:layout_width="201dp"
                        android:layout_height="52dp"
                        android:background="@drawable/spinner_background"
                        android:entries="@array/months"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.555" />

                    <Spinner
                        android:id="@+id/dateSpinner"
                        android:layout_width="100dp"
                        android:layout_height="52dp"
                        android:background="@drawable/spinner_background"
                        android:entries="@array/days"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.68"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias=".555" />

                    <Spinner
                        android:id="@+id/timeSpinner"
                        android:layout_width="90dp"
                        android:layout_height="52dp"
                        android:background="@drawable/spinner_background"
                        android:entries="@array/time"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.555" />

                    <TextView
                        android:id="@+id/whenText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/nunito"
                        android:text="@string/when_do_you_need_it_fixed"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.47" />

                    <TextView
                        android:id="@+id/contactText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/nunito"
                        android:text="@string/who_should_they_ask_for"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.662" />

                    <TextView
                        android:id="@+id/phoneNumberText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/nunito"
                        android:text="@string/what_s_their_phone_number"
                        android:textAllCaps="true"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.862" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/restaurantLocation"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textAlignment="center"
                    android:textColor="#6D7377"
                    android:textSize="21sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.050000012" />

                <TextView
                    android:id="@+id/errorMessage"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:fontFamily="@font/nunito_sans"
                    android:text=""
                    android:textAlignment="center"
                    android:textColor="#CA2500"
                    android:textSize="21sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.121" />

                <ImageView
                    android:id="@+id/greenBar"
                    android:layout_width="75dp"
                    android:layout_height="3dp"
                    android:contentDescription="@string/green_bar"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.100000024"
                    app:srcCompat="@color/colorAccent" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/jobDetailTitleLayout"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@drawable/no_elevation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:visibility="visible">

        <TextView
            android:id="@+id/jobDetailTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/describe_the_job"
            android:textSize="27sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/expandBackButton"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:contentDescription="@string/cancel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias=".15"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.50"
            app:srcCompat="@mipmap/chevron_left" />

        <ImageView
            android:id="@+id/collapseBackButton"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:contentDescription="@string/cancel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.50"
            app:srcCompat="@mipmap/chevron_left" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
